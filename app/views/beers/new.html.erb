<h1>Add a Beer</h1>

<%= form_for @beer do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :style %>
  <%= f.text_field :style %>

  <%= f.label :abv %>
  <%= f.number_field :abv %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit :"Add Beer" %>
<% end %>
